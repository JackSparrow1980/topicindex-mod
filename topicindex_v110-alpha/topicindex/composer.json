{
	"name": "dmzx/topicindex",
	"type": "phpbb-extension",
	"description": "Create a ordered list of topics in a post and in external page.",
	"homepage": "https://www.dmzx-web.net",
	"version": "1.1.0-alpha",
	"time": "2024-11-23",
	"keywords": [
		"phpbb",
		"extension",
		"topicindex"
	],
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "dmzx",
			"homepage": "https://www.dmzx-web.net",
			"email": "info@dmzx-web.net",
			"role": "Extension Developer"
		},
		{
			"name": "Soshen",
			"homepage": "http://www.nipponart.org",
			"role": "Original MOD author"
		},
		{
			"name": "JackSparrow",
			"homepage": "https://www.italiancoverdesign.com",
			"role": "Contributor - UI and Style Improvements"
		}
	],
	"require": {
		"php": ">=5.3.3",
		"composer/installers": "~1.0"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
	},
	"extra": {
		"display-name": "Topic Index",
